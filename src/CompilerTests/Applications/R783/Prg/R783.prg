#pragma options("vo7", ON)
FUNCTION Start( ) AS VOID
	LOCAL oValue AS OBJECT
	LOCAL r8 AS REAL8
	LOCAL r4 AS REAL4
	LOCAL i  AS INT
	LOCAL i64 AS INT64
	LOCAL d AS DWORD
	LOCAL u AS UInt64           
    LOCAL de AS Decimal
	oValue := 1
	i :=  oValue             
    r8 := oValue
	r4 := oValue
	i64 := oValue
	d :=  oValue
    u :=  oValue	
	de := oValue
	? i
	? r8 
	? r4 
	? i 
	? i64
	? d 
	? u
	? de  
	
RETURN
